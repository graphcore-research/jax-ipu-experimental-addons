{
    "name": "JAX-IPU-Python3",
    "build": {
        "dockerfile": "Dockerfile",
        "context": "..",
        "args": {
            "BUILDKIT_INLINE_CACHE": "0",
            "USERNAME": "vscode"
       }
    },
    "forwardPorts": [
        3000,
        6006
    ],
    "runArgs": [
        "--gpus",
        "all"
    ],
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-vscode.cpptools-extension-pack",
        "xaver.clang-format"
    ]
}
